{
    "name": "node-chat-engine",
    "version": "0.0.1",
    "description": "My own chat engine based on Node!",
    "main": "app.js",
    "scripts": {
        "build:admin": "webpack --colors --progress --config admin.config.js",
        "build:chat": "webpack --colors --progress --config chat.config.js",
        "start": "node app",
        "start:testdb": "NODE_ENV=test node app",
        "stop": "pkill --signal SIGINT NodeChatEngine",
        "storybook": "start-storybook -p 9001 -c chat/.storybook",
        "storybook:build": "build-storybook -c chat/.storybook -o react/.out",
        "dbsetup": "node ./db/takedown && node ./db/setup",
        "dbtakedown": "node ./db/takedown",
        "dbsetup:test": "NODE_ENV=test node ./db/setup",
        "dbtakedown:test": "NODE_ENV=test node ./db/takedown",
        "cy:run": "cypress run",
        "cy:run:local": "yarn dbsetup:test && cypress run --headed && yarn dbtakedown:test",
        "test": "nyc mocha --exit test/**.js",
        "test:local": "yarn dbsetup:test && nyc mocha --exit test/**.js && yarn dbtakedown:test"
    },
    "keywords": [
        "node",
        "portal",
        "server",
        "feersum",
        "websockets"
    ],
    "author": "David Oberholzer",
    "license": "ISC",
    "dependencies": {
        "@material-ui/core": "^1.2.1",
        "@material-ui/icons": "^1.1.0",
        "admin-on-rest": "^1.4.0",
        "body-parser": "^1.18.2",
        "cors": "^2.8.4",
        "express": "^4.16.2",
        "express-ws": "^3.0.0",
        "fs": "0.0.1-security",
        "http": "0.0.0",
        "nyc": "^12.0.2",
        "pg": "^7.4.3",
        "react": "^16.3.0",
        "react-addons-css-transition-group": "^15.6.2",
        "react-dom": "^16.3.0",
        "react-redux": "^5.0.7",
        "react-router": "^4.3.1",
        "react-router-redux": "^4.0.8",
        "redux": "^4.0.0",
        "request": "^2.87.0",
        "sha256": "^0.2.0",
        "uuid": "^3.2.1"
    },
    "devDependencies": {
        "@storybook/addon-actions": "^3.3.14",
        "@storybook/addon-info": "^3.3.14",
        "@storybook/cli": "^3.3.14",
        "@storybook/react": "^3.3.14",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.4",
        "babel-plugin-add-module-exports": "0.2.1",
        "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
        "babel-plugin-transform-es3-property-literals": "^6.22.0",
        "babel-plugin-transform-object-rest-spread": "^6.23.0",
        "babel-plugin-transform-react-jsx": "^6.24.1",
        "babel-preset-env": "^1.5.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "chai": "^4.1.2",
        "chai-as-promised": "^7.1.1",
        "css-loader": "^0.28.10",
        "cypress": "^3.0.1",
        "extract-text-webpack-plugin": "^3.0.2",
        "import-glob-loader": "^1.1.0",
        "mocha": "^5.2.0",
        "node-sass": "^4.7.2",
        "postcss": "^6.0.19",
        "postcss-loader": "^2.1.0",
        "sass-loader": "^6.0.6",
        "style-loader": "^0.20.2",
        "webpack": "^3.11.0",
        "webpack-dashboard": "^0.4.0",
        "webpack-dev-server": "^2.5.1",
        "webpack-node-externals": "^1.6.0"
    },
    "nyc": {
        "exclude": [
            "./db/control.js"
        ]
    }
}
